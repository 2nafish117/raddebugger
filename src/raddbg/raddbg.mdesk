  {RD_CfgID          dst_panel        DstPanel      }
  {RD_CfgID          cfg              Cfg           }
  {RD_CfgIDList      cfg_list         CfgList       }
  
  // rjf: frame selection
  {U64               unwind_count     UnwindCount   }
